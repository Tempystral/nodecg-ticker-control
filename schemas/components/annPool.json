{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "priority": {
            "type": "number"
        },
        "announcements": {
            "type": "array",
            "items": {
                "$ref": "annRef.json"
            }
        }
    },
    "required": [
        "name",
        "priority",
        "announcements"
    ],
    "default": {
        "name": "New",
        "priority": 0,
        "announcements": []
    }
}